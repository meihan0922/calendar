{
  "name": "webpack-config",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open --colors --progress --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "sb": "start-storybook -s ./public/json -p 9001 -c .storybook",
    "lint": "eslint \"./src/**/*.{js,jsx}\" --fix"
  },
  "pre-commit": [
    "lint"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/transform-runtime"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@storybook/addon-actions": "^4.1.9",
    "@storybook/addon-knobs": "^4.1.6",
    "@storybook/addon-links": "^4.1.9",
    "@storybook/addon-notes": "^4.1.9",
    "@storybook/addons": "^4.1.9",
    "@storybook/react": "^4.1.4",
    "autoprefixer": "^9.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-react": "^7.12.3",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
